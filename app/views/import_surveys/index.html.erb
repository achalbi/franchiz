<p id="notice"><%= notice %></p>

<h1>Listing Import Surveys</h1>

<table>
  <thead>
    <tr>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @import_surveys.each do |import_survey| %>
      <tr>
        <td><%= link_to 'Show', import_survey %></td>
        <td><%= link_to 'Edit', edit_import_survey_path(import_survey) %></td>
        <td><%= link_to 'Destroy', import_survey, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Import survey', new_import_survey_path %>
