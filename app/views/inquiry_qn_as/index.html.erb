<p id="notice"><%= notice %></p>

<h1>Listing Inquiry Qnas</h1>

<table>
  <thead>
    <tr>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @inquiry_qnas.each do |inquiry_qna| %>
      <tr>
        <td><%= link_to 'Show', inquiry_qna %></td>
        <td><%= link_to 'Edit', edit_inquiry_qna_path(inquiry_qna) %></td>
        <td><%= link_to 'Destroy', inquiry_qna, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

