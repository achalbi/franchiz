<p id="notice"><%= notice %></p>

<h4>Import Survey Templates</h4>

<table>
  <thead>
    <tr>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @import_survey_templates.each do |import_survey_template| %>
      <tr>
        <td><%= link_to 'Show', import_survey_template %></td>
        <td><%= link_to 'Edit', edit_import_survey_template_path(import_survey_template) %></td>
        <td><%= link_to 'Destroy', import_survey_template, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

