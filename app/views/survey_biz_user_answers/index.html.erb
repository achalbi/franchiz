<p id="notice"><%= notice %></p>

<h1>Listing Survey Biz User Answers</h1>

<table>
  <thead>
    <tr>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @survey_biz_user_answers.each do |survey_biz_user_answer| %>
      <tr>
        <td><%= link_to 'Show', survey_biz_user_answer %></td>
        <td><%= link_to 'Edit', edit_survey_biz_user_answer_path(survey_biz_user_answer) %></td>
        <td><%= link_to 'Destroy', survey_biz_user_answer, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

