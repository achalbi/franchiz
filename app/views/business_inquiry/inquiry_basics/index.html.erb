<p id="notice"><%= notice %></p>

<h1>Listing Business Inquiry Basics</h1>

<table>
  <thead>
    <tr>
      <th>Req token</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @business_inquiry_basics.each do |business_inquiry_basic| %>
      <tr>
        <td><%= business_inquiry_basic.req_token %></td>
        <td><%= link_to 'Show', inquiry_basic_path(inquiry_basic_path(business_inquiry_basic)) %></td>
        <td><%= link_to 'Edit', edit_inquiry_basic_path(business_inquiry_basic) %></td>
        <td><%= link_to 'Destroy',inquiry_basic_path(business_inquiry_basic), method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

