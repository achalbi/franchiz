
<div class="col-md-12">
<p>
  <strong>Inquiry no:</strong>
  <%= @inquiry.token %>
</p>
<div id="question" class="mb20" ></div>
<div>
<table class="table table-hover">
  <thead class="thead-default">
    <tr>
      <th>#</th>
      <th>Title</th>
    </tr>
  </thead>
  <tbody>
    
      <% @inquiry.inquiry_answers.each_with_index do |a, i| %>
      
      <tr>
        <th scope="row"><%= i+1 %>. </th>
        <td><%= a.inquiry_question.title %><br>
           <div class="text-success"> <%= a.answer %> </div>
            
            
        </td>
      </tr>

      <% end %>

  </tbody>
</table>
</div>



